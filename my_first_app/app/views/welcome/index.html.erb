<h1>Welcome to My Rails App</h1>
<p>This is a paragraph in Rails view file.</p>
<p>Current Time: <%= Time.now %></p>

<%= form_with(url: '/submit', method: 'post') do |form| %>
  <%= form.label :username, "Username:" %><br>
  <%= form.text_field :username %><br>
  <%= form.label :message, "Message:" %><br>
  <%= form.text_area :message %><br>
  <%= form.submit "Submit" %>
<% end %>
